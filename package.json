{
  "name": "gatsby-starter-tailwind",
  "description": "Gatsby starter styled with Tailwind",
  "version": "4.0.0",
  "author": "Taylor Bryant <taylor@bryant.io>",
  "dependencies": {
    "gatsby": "2.24.58",
    "gatsby-plugin-manifest": "2.4.29",
    "gatsby-plugin-offline": "3.2.27",
    "gatsby-plugin-postcss": "2.3.11",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-sharp": "2.6.35",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "^6.1.0"
  },
  "keywords": [
    "gatsby",
    "tailwind",
    "tailwindcss",
    "purgecss"
  ],
  "license": "MIT",
  "scripts": {
    "develop": "gatsby develop",
    "start": " npm run watch:css && develop",
    "build": " npm run build:css && develop",
    "build:css": "postcss src/css/tailwind.css -o src/build/tailwind.css",
    "watch:css": "postcss src/css/tailwind.css -o src/build/tailwind.css",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
